{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": { "cache-control": "s-maxage=31536000, immutable" }
    }
  ],
  "static": {
    "directory": "public"
  }
}
